{
    "name": "codezero/laravel-form-field-prefixer",
    "description": "Reuse form partials and automatically add optional prefixes and array keys to your form fields.",
    "keywords": [
        "php",
        "laravel",
        "form",
        "prefix"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ivan Vermeyen",
            "email": "ivan@codezero.be"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0|^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^9.0"
    },
    "scripts": {
        "test": "phpunit"
    },
    "autoload": {
        "psr-4": {
            "CodeZero\\FormFieldPrefixer\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "CodeZero\\FormFieldPrefixer\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "aliases": {
                "FormFieldPrefixer": "CodeZero\\FormFieldPrefixer\\Facades\\FormFieldPrefixer"
            }
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
